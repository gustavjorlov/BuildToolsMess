{
  "name": "KitsCon16",
  "version": "1.3.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist_demo/*",
    "lint": "eslint src/** --quiet",
    "precompile": "npm run lint",
    "compile": "browserify ./src/app --debug -t babelify -o dist_demo/bundle.js",
    "copy:images": "cp -R src/images dist_demo/",
    "copy:style": "cp -R src/style dist_demo/",
    "copy:html": "cp src/index.html dist_demo/",
    "copy": "npm run copy:images && npm run copy:style && npm run copy:html",
    "watch:js": "watch 'npm run compile' src",
    "browsersync": "browser-sync start --files dist_demo/bundle.js --server dist_demo",
    "prebuild": "rimraf dist_demo && mkdir dist_demo",
    "build": "npm run compile && npm run copy",
    "build:clean": "npm run clean && npm run build",
    "dev": "npm run browsersync & npm run watch:js",
    "deploy": "npm run clean && npm run build && npm version patch && git push origin master && git push heroku master"
  },
  "engines": {
    "node": "~4.2.1",
    "npm": "~3.7.0"
  },
  "keywords": [],
  "author": "Gustav Jorl√∂v",
  "license": "ISC",
  "dependencies": {
    "express": "^4.13.4",
    "gulp": "3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-serve": "^1.2.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "del": "^2.2.0",
    "eslint": "^2.2.0",
    "grunt": "^0.4.5",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^4.0.1",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.2",
    "jscs": "^2.11.0",
    "load-grunt-tasks": "^3.4.1",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watch": "^0.17.1"
  }
}
