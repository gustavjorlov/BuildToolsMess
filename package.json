{
  "name": "KitsCon16",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:prod && node index.js",
    "clean": "rimraf dist_npm/*",
    "lint": "eslint src/** --quiet",
    "precompile:prod": "npm run lint",
    "precompile:dev": "npm run lint",
    "compile:prod": "browserify ./src/app -t babelify | uglifyjs -mc > dist_npm/bundle.js",
    "compile:dev": "browserify ./src/app --debug -t babelify -o dist_npm/bundle.js",
    "copy:images": "cp -R src/images dist_npm/",
    "copy:style": "cp -R src/style dist_npm/",
    "copy:html": "cp src/index.html dist_npm/",
    "copy": "npm run copy:images && npm run copy:style && npm run copy:html",
    "watch:js": "watch 'npm run compile:dev' src",
    "browsersync": "browser-sync start --files dist_npm/bundle.js --server dist_npm",
    "prebuild:dev": "rimraf dist_npm && kdir dist_npm",
    "prebuild:prod": "rimraf dist_npm && mkdir dist_npm",
    "build:dev": "time(npm run compile:dev && npm run copy)",
    "build:prod": "time(npm run compile:prod && npm run copy)",
    "build:clean": "npm run clean && npm run build:dev",
    "dev": "npm run browsersync & npm run watch:js",
    "deploy": "npm run clean && npm run build:prod",
    "deploy:patch": "npm run deploy && npm version patch && git push",
    "deploy:minor": "npm run deploy && npm version minor && git push"
  },
  "engines": {
    "node": "~4.2.1",
    "npm": "~3.7.0"
  },
  "keywords": [],
  "author": "Gustav Jorl√∂v",
  "license": "ISC",
  "dependencies": {
    "express": "^4.13.4",
    "gulp": "3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-serve": "^1.2.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "del": "^2.2.0",
    "eslint": "^2.2.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.2",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watch": "^0.17.1"
  }
}
